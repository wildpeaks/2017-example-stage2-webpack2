{
  "private": true,
  "name": "example",
  "description": "ES Modules, Stage2 and Webpack",
  "version": "0.0.1",
  "engines": {
    "node": ">=7.8.0"
  },
  "eslintConfig": {
    "extends": "@wildpeaks/esmodules-stage2"
  },
  "scripts": {
    "reset": "rimraf www/* || true",
    "start": "webpack-dev-server || true",

    "build:webpack": "webpack --env.minify",
    "build": "npm run reset && npm run build:webpack",

    "analyze:build": "webpack --env.minify --profile --json > www/webpack.stats.json",
    "analyze:view": "webpack-bundle-analyzer www/webpack.stats.json --mode static --report www/webpack.stats.html",
    "analyze": "npm run reset && npm run analyze:build && npm run analyze:view",

    "lint": "eslint src"
  },
  "author": "Cecile Muller",
  "dependencies": {

    "babel-core": "7.0.0-alpha.7",
    "babel-loader": "7.0.0-beta.1",
    "babel-register": "7.0.0-alpha.7",
    "babel-plugin-transform-class-properties": "7.0.0-alpha.7",
    "babel-plugin-transform-runtime": "7.0.0-alpha.7",
    "babel-preset-env": "2.0.0-alpha.4",
    "babel-preset-flow": "7.0.0-alpha.7",

    "css-loader": "0.28.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "postcss-cssnext": "2.10.0",
    "postcss-loader": "1.3.3",
    "rimraf": "2.6.1",
    "style-loader": "0.16.1",
    "url-loader": "0.5.8",
    "webpack": "2.3.3",
    "webpack-dev-server": "2.4.2",
    "webpack-subresource-integrity": "0.9.0"
  },
  "devDependencies": {
    "@wildpeaks/eslint-config-esmodules-stage2": "2.2.0",
    "eslint": "3.19.0",
    "mocha": "3.2.0",
    "webpack-bundle-analyzer": "2.4.0"
  }
}
